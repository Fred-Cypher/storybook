<form method="post" action="{{ path('app_games_delete_recent_game', {'id': recentGame.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer ce jeu ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ recentGame.id) }}">
    <button class="btn">Supprimer le jeu</button>
</form>
